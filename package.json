{
	"name": "base-reactnative",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "react-native start",
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"test": "jest",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"dev": "yarn start",
		"upgrade-packages": "yarn upgrade && yarn code-check",
		"reset-modules": "rm -rf node_modules && rm -rf dist && rm -rf out && rm -rf .cache && yarn install",
		"link": "npx react-native link",
		"code-check": "yarn lint && yarn tsc",
		"tsc": "npx tsc -noEmit",
		"release-ios": "react-native run-ios --variant=release",
		"iPhone11ProMax": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
		"iPhone8Plus": "react-native run-ios --simulator=\"iPhone 8 Plus\"",
		"iPhone11Pro": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
		"ipp": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
		"release-android": "react-native run-android --variant=release",
		"pixel-28": "emulator @Pixel_API_28",
		"pixel": "emulator @Pixel",
		"options": "adb shell input keyevent 82",
		"android-key": "cd android/app/keys && keytool -genkey -v -keystore debug.keystore -storepass android -alias androiddebugkey -keypass android -keyalg RSA -keysize 2048 -validity 10000 && cd ../../../"
	},
	"dependencies": {
		"@react-native-community/masked-view": "^0.1.10",
		"@react-navigation/native": "^5.1.6",
		"@react-navigation/stack": "^5.2.13",
		"axios": "^0.21.1",
		"install": "^0.13.0",
		"localized-strings": "^0.2.4",
		"metro-config": "^0.59.0",
		"native-base": "^2.15.2",
		"react": "16.11.0",
		"react-native": "0.62.2",
		"react-native-dotenv": "^2.5.5",
		"react-native-gesture-handler": "^1.6.1",
		"react-native-reanimated": "^1.8.0",
		"react-native-safe-area-context": "^0.7.3",
		"react-native-screens": "^2.7.0",
		"react-native-svg": "^12.1.0",
		"react-redux": "^7.2.4",
		"reactotron-react-native": "^5.0.0",
		"reactotron-redux": "^3.1.3",
		"reactotron-redux-saga": "^4.2.3",
		"redux": "^4.1.0",
		"typesafe-actions": "^5.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/plugin-proposal-decorators": "^7.8.3",
		"@babel/runtime": "^7.6.2",
		"@react-native-community/eslint-config": "^1.0.0",
		"@types/jest": "^24.0.24",
		"@types/react-native": "^0.62.0",
		"@types/react-redux": "^7.1.16",
		"@types/react-test-renderer": "16.9.2",
		"@types/redux-saga": "^0.10.5",
		"@typescript-eslint/eslint-plugin": "^2.27.0",
		"@typescript-eslint/parser": "^2.27.0",
		"babel-jest": "^24.9.0",
		"eslint": "^6.5.1",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^2.5.0",
		"jest": "^24.9.0",
		"metro-react-native-babel-preset": "^0.58.0",
		"prettier": "^2.0.4",
		"react-native-svg-transformer": "^0.14.3",
		"react-test-renderer": "16.11.0",
		"typescript": "^3.8.3"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
